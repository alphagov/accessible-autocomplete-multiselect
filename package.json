{
  "name": "accessible-autocomplete-multi-select",
  "version": "1.6.2",
  "main": "dist/accessible-autocomplete.min.js",
  "style": "dist/accessible-autocomplete.min.css",
  "description": "An autocomplete component, built to be accessible.",
  "repository": "alphagov/accessible-autocomplete",
  "author": "Government Digital Service (https://www.gov.uk/government/organisations/government-digital-service)",
  "license": "MIT",
  "keywords": [
    "a11y",
    "accessibility",
    "autocomplete",
    "component",
    "plugin",
    "typeahead",
    "widget"
  ],
  "scripts": {
    "build:css": "csso src/autocomplete.css -o dist/accessible-autocomplete.min.css",
    "build:js": "cross-env NODE_ENV=production webpack --progress --display-modules",
    "build": "run-s 'build:js' 'build:css'",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --progress",
    "karma:dev": "cross-env NODE_ENV=test karma start test/karma.config.js",
    "karma": "npm run karma:dev -- --single-run",
    "preversion": "npm test",
    "standard": "standard",
    "test": "run-p standard karma wdio",
    "version": "npm run build && git add -A dist",
    "wdio": "cross-env NODE_ENV=test wdio test/wdio.config.js && git checkout dist/"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run build && node scripts/check-staged.js"
    }
  },
  "dependencies": {
    "preact": "^8.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-transform-member-expression-literals": "^7.22.5",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/plugin-transform-property-literals": "^7.0.0",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@babel/preset-env": "^7.23.2",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-istanbul": "^5.1.0",
    "chai": "^4.2.0",
    "chalk": "^5.3.0",
    "copy-webpack-plugin": "^4.6.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "csso-cli": "^1.1.0",
    "dotenv": "^6.1.0",
    "husky": "^1.1.3",
    "karma": "^6.4.1",
    "karma-chai": "^0.1.0",
    "karma-chai-sinon": "^0.1.5",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^5.0.0-alpha.4",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^1.10.0",
    "replace-bundle-webpack-plugin": "^1.0.0",
    "sinon": "^6.3.5",
    "sinon-chai": "^3.2.0",
    "source-map-loader": "^0.2.4",
    "standard": "^17.1.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "wdio-mocha-framework": "^0.6.4",
    "wdio-sauce-service": "^0.4.13",
    "wdio-selenium-standalone-service": "^0.0.10",
    "wdio-spec-reporter": "^0.1.5",
    "wdio-static-server-service": "^1.0.1",
    "wdio-webpack-dev-server-service": "^2.0.2",
    "webdriverio": "^8.8.4",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-dev-server": "^3.1.10",
    "webpack-sources": "^3.2.3"
  },
  "browserslist": [
    ">0.1%",
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Edge versions",
    "last 2 Samsung versions",
    "Safari >= 9",
    "ie 8-11",
    "iOS >= 9"
  ],
  "standard": {
    "parser": "babel-eslint"
  }
}
